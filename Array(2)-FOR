Input :

fun main(){
    val ECE = arrayOf("Signal System","VLSI","Computer Network","Embedded System Design")
    val CSE = arrayOf("Data Structure","Design Algorithms","DBMS","Computer Network")
    for(ECE in ECE){
        println(ECE)
    }
    for(CSE in CSE){
        println(CSE)
    }

}

